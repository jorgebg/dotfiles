[global_config]
  inactive_color_offset = 1.0
[keybindings]
  new_tab = <Primary>t
  paste = <Primary>v
  search = <Primary>f
  next_tab = <Primary>Tab
  prev_tab = <Primary><Shift>Tab
[profiles]
  [[default]]
    cursor_color = "#aaaaaa"
    font = Fira Mono 14
    foreground_color = "#eeeeec"
    use_system_font = False
    copy_on_selection = True
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
